@using TripAdvisor.Models
@{
  Layout = "_Layout";
}

<div class="bg-panel">
  <h1>@Country.Find(Model[0].GetCountryId()).GetName()'s City List</h1>

  <ul class="no-bullet">
    @foreach (var city in Model)
    {
      <li><a href="/Cities/@city.GetId()">@city.GetName()</a></li>
    }
  </ul>
</div>
